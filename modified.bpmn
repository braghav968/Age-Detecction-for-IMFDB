      </bpmn:extensionElements>
      <bpmn:incoming>Flow_14qex3h</bpmn:incoming>
      <bpmn:outgoing>Flow_1ffrp1r</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0lu71l3" sourceRef="scheduleAppointment" targetRef="waitTillAppointmentDate" />
  </bpmn:process>
